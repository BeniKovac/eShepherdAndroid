<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    android:layout_marginTop="16dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginBottom="16dp"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/DatumKotitveTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/datum_kotitve"
        android:textStyle="bold"
        android:textSize="8pt"/>

    <TextView
        android:id="@+id/DatumKotitve"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/SteviloMladihTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/stevilo_mladih"
        android:textStyle="bold"
        android:textSize="8pt"/>

    <TextView
        android:id="@+id/SteviloMladih"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/OvcaIDtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/id_ovce"
        android:textStyle="bold"
        android:textSize="8pt"/>

    <TextView
        android:id="@+id/OvcaID"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/OvenIDtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/id_ovna"
        android:textStyle="bold"
        android:textSize="8pt" />

    <TextView
        android:id="@+id/OvenID"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/SteviloMrtvihTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tevilo_mrtvorojenih"
        android:textStyle="bold"
        android:textSize="8pt"/>

    <TextView
        android:id="@+id/SteviloMrtvih"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/OpombeTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="@string/opombe"
        android:textSize="8pt"/>

    <TextView
        android:id="@+id/Opombe"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />




</LinearLayout>
